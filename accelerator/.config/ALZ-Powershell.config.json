        "AzFirewallName": {
            "Type": "Computed",
            "Value": "alz-azfw-{%Location%}",
            "Targets": [
                {
                    "Name": "parAzFirewallName.value",
                    "Destination": "Parameters"
                }
            ]
        },
        "FirewallPoliciesName": {
            "Type": "Computed",
